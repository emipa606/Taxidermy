  <ThingDef ParentName="TaxidermyBase" Name="TaxidermySquirrelBase" Abstract="True">
    <label>stuffed squirrel</label>
    <description>A stuffed squirrel.</description>
    <designatorDropdown>TaxidermySquirrelGroup</designatorDropdown>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Squirrel/Squirrel</texPath>
      <drawSize>1.5</drawSize>
      <color>(161,95,38)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <Mass>2.5</Mass>
      <Beauty>25</Beauty>
      <WorkToBuild>3500</WorkToBuild>
    </statBases>
    <size>(1,1)</size>
  </ThingDef>
  <ThingDef ParentName="TaxidermySquirrelBase">
    <defName>TaxidermySquirrel</defName>
    <costList>
      <Leather_Light>50</Leather_Light>
      <WoodLog>15</WoodLog>
    </costList>
  </ThingDef>
  <ThingDef ParentName="TaxidermySquirrelBase">
