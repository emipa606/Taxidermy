  <ThingDef ParentName="TaxidermyBase" Name="TaxidermyCatBase" Abstract="True">
    <label>stuffed cat</label>
    <description>A stuffed cat.</description>
    <designatorDropdown>TaxidermyCatGroup</designatorDropdown>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Cat/Cat</texPath>
      <drawSize>1.0</drawSize>
      <color>(189,171,159)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <Mass>2.5</Mass>
      <Beauty>25</Beauty>
      <WorkToBuild>3500</WorkToBuild>
    </statBases>
    <size>(1,1)</size>
  </ThingDef>
